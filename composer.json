{
    "name":        "auth0/jwt-auth-bundle",
    "type":        "symfony-bundle",
    "description": "Support for authenticating users JWT.",
    "keywords":    [
        "authentication",
        "firewall",
        "jwt",
        "security",
        "auth0"
    ],
    "homepage": "http://github.com/auth0/jwt-auth-bundle",
    "license": "MIT",

    "authors": [
        {
            "name": "Germ√°n Lena",
            "email": "german.lena@gmail.com"
        }
    ],

    "require": {
        "php": "^5.5 || ^7.0",
        "symfony/framework-bundle": "^2.8 || ~3.0",
        "auth0/auth0-php": "^5.0.3"
    },

    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "nyholm/symfony-bundle-test": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Auth0\\JWTAuthBundle\\": "src/"
        }
    }
    "autoload-dev": {
        "psr-4": {
            "Auth0\\JWTAuthBundle\\": "Tests/"
        }
    }
}
