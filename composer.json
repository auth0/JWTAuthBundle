{
    "name":        "auth0/jwt-auth-bundle",
    "type":        "symfony-bundle",
    "description": "Support for authenticating users with a JWT in Symfony 4",
    "keywords":    [
        "authentication",
        "firewall",
        "jwt",
        "security",
        "auth0"
    ],
    "homepage": "http://github.com/auth0/jwt-auth-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Cunningham",
            "email": "josh.cunningham@auth0.com"
        }
    ],

    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^4.0",
        "auth0/auth0-php": "^5.4"
    },

    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "nyholm/symfony-bundle-test": "^1.0",
        "symfony/security": "^4.0"
    },

    "autoload": {
        "psr-4": {
            "Auth0\\JWTAuthBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Auth0\\JWTAuthBundle\\": "Tests/"
        }
    }
}
